<% @body_class= "session-new"%>
<h1 id= "rotten-mangoes-hdr">Log In</h1>
<%= form_tag session_path do %>
<div class="session-new-login">
  <div>
    <%= label_tag "email" %><br/>
    <%= text_field_tag "email" %>
  </div>
  <div>
    <%= label_tag :password %><br/>
    <%= password_field_tag :password %>
  </div>
  <div><%= submit_tag "Log In", class: "btn btn-primary", id:"login-btn" %></div>
</div>
<% end %>