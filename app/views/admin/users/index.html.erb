<h1 id="rotten-mangoes-hdr">List of all Users</h1>
  <div class="admin-create-user">
    <%= link_to "Create New User", new_admin_user_path %>
  </div>
  <% @users.each do |user| %>
  <div class="admin-list-users">
    <%= user.full_name %> /
    <%= user.email %> /
    <%= user.admin %> /
    <%= link_to "Edit", edit_admin_user_path(user) %> / <%= link_to "Delete", admin_user_path(user), method: :delete, confirm: "You sure?" %>
  </div>
  <% end %>